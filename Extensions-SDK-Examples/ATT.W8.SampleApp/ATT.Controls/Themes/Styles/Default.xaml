                            <RepeatButton x:Name="VerticalSmallIncrease" HorizontalAlignment="Center" Height="20" IsTabStop="False" Interval="50" Margin="0" MinWidth="17" Grid.Row="4" Template="{StaticResource VerticalIncrementTemplate}"/>
